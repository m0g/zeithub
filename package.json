{
  "name": "zeithub",
  "version": "0.2.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "16.13.1",
    "npm": "8.1.2"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' ./src/server/index.ts",
    "dev": "nodemon",
    "tsc": "tsc --module \"commonjs\" --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build": "npm run webpack:prod && npm run tsc",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/vue-fontawesome": "2.0.8",
    "@vue/server-renderer": "^3.2.41",
    "bcrypt": "5.0.1",
    "cookie-parser": "1.4.6",
    "db-migrate": "1.0.0-beta.16",
    "db-migrate-mysql": "^2.1.1",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "6.0.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "luxon": "3.0.3",
    "morgan": "^1.10.0",
    "mysql2": "2.3.3",
    "puppeteer-core": "chrome-93",
    "slugify": "^1.6.5",
    "vue": "3.2.41",
    "vue-class-component": "^7.2.5",
    "vue-imask": "6.0.5",
    "vue-property-decorator": "9.1.2",
    "vue-router": "^3.4.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "4.1.3",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.1",
    "@types/morgan": "^1.9.1",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.8",
    "babel-minify-webpack-plugin": "^0.3.1",
    "cpx": "^1.5.0",
    "css-loader": "6.7.1",
    "eslint": "8.23.0",
    "expect": "29.0.2",
    "mocha": "10.0.0",
    "nodemon": "2.0.19",
    "postcss": "^8.4.16",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "tailwindcss": "^3.1.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "typescript": "4.4.4",
    "vue-loader": "15.9.8",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.6.14",
    "webpack": "5.74.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "5.3.3",
    "webpack-merge": "5.8.0"
  }
}
