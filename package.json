{
  "name": "zeithub",
  "version": "0.2.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "16.13.1",
    "npm": "8.1.2"
  },
  "scripts": {
    "start": "node ./build/server/index.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' ./src/server/index.ts",
    "dev": "nodemon",
    "tsc": "tsc --module \"commonjs\" --outDir build/",
    "posttsc": "cpx 'src/**/*.js' build/",
    "lint": "eslint src/",
    "pretest": "db-migrate up --config config/database.json -e test",
    "test": "NODE_ENV=test mocha -r ts-node/register src/**/*.test.ts",
    "posttest": "db-migrate reset --config config/database.json -e test",
    "db:up": "db-migrate up --config config/database.json",
    "db:down": "db-migrate down --config config/database.json",
    "db:reset": "db-migrate reset --config config/database.json",
    "webpack:prod": "webpack --config webpack.prod.js",
    "build": "npm run webpack:prod && npm run tsc",
    "heroku-postbuild": "db-migrate up && webpack --config webpack.prod.js && npm run tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.2",
    "@vue/server-renderer": "^3.2.45",
    "bcrypt": "5.1.0",
    "cookie-parser": "1.4.6",
    "db-migrate": "1.0.0-beta.16",
    "db-migrate-mysql": "^2.2.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-history-api-fallback": "^2.2.1",
    "helmet": "6.0.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "8.5.1",
    "luxon": "3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "2.3.3",
    "pinia": "^2.0.23",
    "puppeteer-core": "19.2.2",
    "slugify": "^1.6.5",
    "vue": "3.2.45",
    "vue-imask": "6.4.3",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.14",
    "@types/mocha": "10.0.0",
    "@types/morgan": "^1.9.3",
    "@vitejs/plugin-vue": "^3.2.0",
    "assert": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "cpx": "^1.5.0",
    "css-loader": "6.7.1",
    "eslint": "8.27.0",
    "expect": "29.3.1",
    "mocha": "10.1.0",
    "nodemon": "2.0.20",
    "postcss": "^8.4.19",
    "postcss-loader": "7.0.1",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "tailwindcss": "^3.2.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "vite": "^3.2.3",
    "vue-loader": "17.0.1",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.7.14",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.7.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-middleware": "5.3.3",
    "webpack-merge": "5.8.0"
  }
}
